#include <stdio.h>

int main() {
    int arr[5] = {10, 20, 30, 40, 50};
    int item = 40;
    int found = 0;

    for (int i = 0; i < 5; i++) {
        if (arr[i] == item) {found = i;
            break;
        }
    }

    if (found != -1)
        printf("Element found at index %d\n", found);
        
    else
        printf("Element not found\n");

    return 0;
}

            
